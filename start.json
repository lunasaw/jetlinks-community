// 物模型定义
{
    "functions": [ // 功能定义
        {
            "output": {
                "falseValue": "false",
                "trueText": "是",
                "trueValue": "true",
                "falseText": "否",
                "type": "boolean"
            },
            "async": false,
            "inputs": [
                {
                    "valueType": {
                        "type": "string"
                    },
                    "name": "NVR标识",
                    "id": "playVoice",
                    "expands": {
                        "required": false
                    }
                }
            ],
            "name": "播放视频",
            "id": "playVoice",
            "expands": {}
        }
    ],
    "properties": [ // 属性定义
        {
            "valueType": {
                "round": "HALF_UP",
                "scale": 2,
                "type": "float"
            },
            "name": "温度",
            "id": "temperature",
            "expands": {
                "type": [
                    "read",
                    "write",
                    "report"
                ],
                "source": "device"
            }
        }
    ],
    "events": [ // 事件定义
        {
            "valueType": {
                "type": "object",
                "properties": [
                    {
                        "valueType": {
                            "type": "string"
                        },
                        "name": "地址",
                        "id": "address"
                    },
                    {
                        "valueType": {
                            "tz": "Asia/Shanghai",
                            "format": "yy-mm-dd hh:mm:ss",
                            "type": "date"
                        },
                        "name": "时间",
                        "id": "time"
                    }
                ]
            },
            "name": "火灾告警",
            "id": "alarm_fire",
            "expands": {
                "level": "ordinary"
            }
        }
    ],
    "tags": [ // 标签定义
        {
            "id": "tag",
            "name": "标签",
            "valueType": {
                "type": "string"
            },
            "expands": {
                "type": [
                    "read",
                    "write",
                    "report"
                ]
            },
            "description": "标签"
        }
    ]
}